cd BimWorkplace.Api
dotnet user-secrets init

dotnet user-secrets set "AzureAd:ClientId" "xxxx"// API client id (BIMWorkplace.Api)
dotnet user-secrets set "AzureAd:TenantId" "xxxx"// Directory (tenant) ID
dotnet user-secrets set "AzureAd:Audience" "xxxx" // Application ID URI da API
dotnet user-secrets set "GoogleDrive:ServiceAccountKeyPath" "caminho/do/service-account.json"


